{
  "name": "react-tailwind-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fontsource-variable/montserrat": "^5.2.6",
    "@fontsource/montserrat": "^5.2.6",
    "@reduxjs/toolkit": "^2.9.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.12.0",
    "firebase": "^12.1.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "lucide-react": "^0.526.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:local": "vite --mode development --env-file .env.local",
    "dev:staging": "vite --mode staging --env-file .env.staging",
    "prod": "vite --mode production",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging --env-file .env.staging",
    "build:prod": "vite build --mode production",
    "build:local": "vite build --mode development --env-file .env.local",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "test": "vitest",
    "font-demo": "echo 'Font demo available at /font-demo route'",
    "font-check": "echo 'Checking font installation...' && npm list @fontsource/montserrat @fontsource-variable/montserrat",
    "env:check": "echo 'Environment Variables:' && echo 'VITE_NODE_ENV:' $VITE_NODE_ENV && echo 'VITE_API_BASE_URL:' $VITE_API_BASE_URL",
    "api:local": "VITE_API_OVERRIDE=LOCAL npm run dev",
    "api:production": "VITE_API_OVERRIDE=PRODUCTION npm run dev",
    "api:old": "VITE_API_OVERRIDE=OLD_SERVER npm run dev"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@vitejs/eslint-config-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "netlify-cli": "^23.1.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^5.2.0",
    "vitest": "^1.4.0"
  }
}
